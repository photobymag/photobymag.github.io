---
import ProseCv from "../components/Prose_cv.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
const pageTitle = "About";
const description = "creative beyond picture";
const site_name = "photoby";
import { getEntry } from "astro:content";
const entry = await getEntry("about", "about");
const { Content } = await entry.render();
---

<BaseLayout title={pageTitle} description={description} site_name={site_name} is404Page={false}>
  <ProseCv>
    <Content />
  </ProseCv>
</BaseLayout>